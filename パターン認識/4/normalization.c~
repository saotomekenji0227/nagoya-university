void readfile(FILE *fp,int *w,int *h,int *A){
  int i,j;
  int tmp;
  fscanf(fp,"%d",&w);
  fscanf(fp,"%d",&h);
  A=(int**)malloc(h*sizeof(int*));
  for(i=0;i<h;i++)
    A[i]=(int*)malloc(w*sizeof(int));
  for(i=0;i<h;i++){
    for(j=0;j<w;j++){
      fscanf(fp,"%d",&tmp);
      A[i][j]=tmp;
    }
  }
}

