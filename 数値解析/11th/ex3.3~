#set term X11
set term postscript portrait
set output 'ex3.3.0.5.-.ps'
set title "081531257saotomekenji.ps"  
set size 1.0, 0.7
set xlabel "X"
set ylabel "Y"
set xtics -2, 0.4
set ytics -2, 0.4
set zeroaxis
plot [-2.1:2.1][-2.1:2.1] 'ex3.3.dat' title '(x,y)=(1,0)' with dots
