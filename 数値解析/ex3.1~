#set term X11
set term postscript portrait
set output '081531257saotomekenji.ps'
set title "081531257saotomekenji.ps"  
set size 1.0, 0.7
set xlabel "X"
set ylabel "Y"
set xtics 0, 10
set ytics -3, 3
set zeroaxis
plot [0:10][-3:3] 'ex3.1.dat' title 'y=e^(x^2)-sin(x)' with dots
